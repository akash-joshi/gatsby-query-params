{
  "name": "gatsby-query-params",
  "version": "1.3.0",
  "description": "Get query params of the current URL in Gatsby",
  "author": "akash-joshi",
  "license": "GPLV3",
  "repository": "akash-joshi/gatsby-query-params",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "gatsby",
    "react",
    "query",
    "parameters",
    "params",
    "cra"
  ],
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
